{
    "name" : "Federated repository",
    "workspaces" : {
        "predefined" : ["ws1", "ws2"],
        "default" : "default",
        "allowCreation" : true
    },
    "externalSources" : {
        "mock-source" : {
            "classname" : "org.modeshape.connector.mock.MockConnector",
            "persistentDataAcrossRestarts" : true,
            "cacheable" : false,
            "projections" : [
                "default:/preconfiguredProjection => /doc1"
            ]
        }
    },
    "storage" : {
        "persistence" : {
            "type" : "file",
            "path": "target/persistent_repository/db"
        }
    }
}