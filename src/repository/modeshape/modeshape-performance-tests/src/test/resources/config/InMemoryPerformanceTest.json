{
    "name" : "In Memory Repository",
    "storage" : {
        "persistence" : {
            "type" : "mem"
        }
    },
    "workspaces" : {
        "default" : "default",
        "allowCreation" : true
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : false
        }
    }
}